<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.yurkirill.dagger2retention">

    <application
        android:name=".di.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".ui.HomeActivity"
            android:label="@string/nav_home"
            android:theme="@style/AppTheme.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.DiaryActivity"
            android:label="@string/nav_diary"
            android:theme="@style/AppTheme.Diary" />

        <activity
            android:name=".ui.StatisticActivity"
            android:label="@string/nav_statistic"
            android:theme="@style/AppTheme.Statistic" />

        <activity
            android:name=".ui.ImpexpActivity"
            android:label="@string/nav_impexp" />

        <activity
            android:name=".ui.SettingsActivity"
            android:label="@string/nav_settings"
            android:theme="@style/AppTheme.Setting" />

    </application>

</manifest>
